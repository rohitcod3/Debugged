+                    <div className="w-full md:w-1/3"></div>
+                    <div className="w-full md:w-2/3">
+                        <QuestionForm question={question} />
+                    </div>
+                </div>
+            </div>
+        </div>
+    );
+};
+
+export default EditQues;
